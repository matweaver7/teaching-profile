{
    "compilerOptions": {
      "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "ES6",
        "target": "ES5",
        "jsx": "react",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
          "@enums/*": [
            "src/enums/*"
          ],
        }
    },
    "include": [
      "src"
    ]
  }